    M:read("Ingrese M(x,y)"),
    print("M = ", M),
    N:read("Ingrese N(x,y)"),
    print("N = ", N),
    dM:diff(M,y),
    dN:diff(N,x),
    if dM = dN then
                (F:integrate(dM,x) and F1:diff(F,y) and g:M-F1)
                else print("La ecuaci√≥n no es exacta") and p:1,
    if p#1 then g1:integrate(g,y) and print("K=",F+g1)    
   )$
    
    
